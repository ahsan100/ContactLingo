<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="60dp">

    <Row>
        <!--
                <Key android:codes="114" android:keyLabel="r" android:keyEdgeFlags="left" />
        -->
        <Key android:codes="116" android:keyLabel="t" android:keyEdgeFlags="left" android:isRepeatable="true"/>
        <Key android:codes="121" android:keyLabel="y" android:isRepeatable="true"/>
        <Key android:codes="117" android:keyLabel="u" android:isRepeatable="true"/>
        <Key android:codes="105" android:keyLabel="i" android:isRepeatable="true"/>
        <Key android:codes="111" android:keyLabel="o" android:isRepeatable="true"/>
        <Key android:codes="112" android:keyLabel="p" android:isRepeatable="true"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>
    <Row>
        <!--
                <Key android:codes="102" android:keyLabel="f" android:keyEdgeFlags="left" />
        -->
        <Key android:codes="103" android:keyLabel="g" android:keyEdgeFlags="left" android:isRepeatable="true"/>
        <Key android:codes="104" android:keyLabel="h" android:isRepeatable="true"/>
        <Key android:codes="106" android:keyLabel="j" android:isRepeatable="true"/>
        <Key android:codes="107" android:keyLabel="k" android:isRepeatable="true"/>
        <Key android:codes="108" android:keyLabel="l" android:isRepeatable="true"/>
        <Key android:codes="59" android:keyLabel=";" android:popupCharacters=":" android:isRepeatable="true"/>
        <Key android:codes="59" android:keyLabel="\'" android:popupCharacters=":" android:isRepeatable="true"/>
    </Row>
    <Row>
        <Key android:codes="118" android:keyLabel="v" android:keyEdgeFlags="left" android:isRepeatable="true"/>
        <Key android:codes="98" android:keyLabel="b" android:isRepeatable="true"/>
        <Key android:codes="110" android:keyLabel="n" android:isRepeatable="true"/>
        <Key android:codes="109" android:keyLabel="m" android:isRepeatable="true"/>
        <!--
        figured hiding period under comma is better than showing both,
        that way we can show ? and /
        -->
        <Key android:codes="44" android:keyLabel="," android:popupCharacters="." android:isRepeatable="true"/>
        <Key android:codes="44" android:keyLabel="." android:popupCharacters="." android:isRepeatable="true"/>
        <Key android:codes="47" android:keyLabel="/" android:popupCharacters="\?" android:isRepeatable="true"/>
        <!--
                <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
                    android:keyEdgeFlags="right" android:isRepeatable="true"/>
        -->
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
            android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="#" android:isRepeatable="true"/>
        <Key android:codes="-10" android:keyLabel="&gt;&gt;" android:isRepeatable="true"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="28%p" android:isRepeatable="true" />
        <!--
                <Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="25%p"/>
        -->
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="28%p" android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

</Keyboard>